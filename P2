#include <iostream>
using namespace std;

#define MAX 5

class Stack {
    float stack[MAX];
    int top;

public:
    Stack() {
        top = -1;
    }

    void push(float signal) {
        if (top == MAX - 1) {
            cout << "Stack is FULL!\n";
            return;
        }
        stack[++top] = signal;
    }

    void pop() {
        if (top == -1) {
            cout << "Stack is EMPTY!\n";
            return;
        }
        cout << "Signal: " << stack[top--] << " V\n";
    }

    bool isEmpty() {
        return top == -1;
    }
};

int main() {
    Stack s;

    s.push(3.3);
    s.push(5.0);
    s.push(1.8);

    cout << "Signals in Reverse Order:\n";
    while (!s.isEmpty()) {
        s.pop();
    }

    return 0;
}
